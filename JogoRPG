	static char sextoDesafio1() {
		char sextaQuestao;
		int contador = 0;

		boolean acertou = false;

		List alternativas = new ArrayList();

		alternativas.add("665");
		alternativas.add("4EC");
		alternativas.add("53E");
		alternativas.add("4FF");
		alternativas.add("4BF");

		do {
			Collections.shuffle(alternativas);

			System.out
					.println("Dado o número 2 4 7 6, de base octal, qual o seu correspondente hexadecimal? ");

			System.out.println("a) " + alternativas.get(0));
			System.out.println("b) " + alternativas.get(1));
			System.out.println("c) " + alternativas.get(2));
			System.out.println("d) " + alternativas.get(3));
			System.out.println("e) " + alternativas.get(4));
			System.out.print("Escolha uma alternativa: ");

			sextaQuestao = entrada.next().toLowerCase().charAt(0);

			switch (sextaQuestao) {

			case 'a':
				if (alternativas.get(0).equals("53E")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil. Parabéns!\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil.Boa sorte!\n");
					contador = contador + 1;
				}
				break;

			case 'b':
				if (alternativas.get(1).equals("53E")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil. Parabéns!\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta!\n\n "
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil.Boa sorte!\n");
					contador = contador + 1;
				}
				break;

			case 'c':
				if (alternativas.get(2).equals("53E")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil. Parabéns!\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil.Boa sorte!\n");
					contador = contador + 1;
				}
				break;

			case 'd':
				if (alternativas.get(3).equals("53E")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil. Parabéns!\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil.Boa sorte!\n");
					contador = contador + 1;
				}
				break;

			case 'e':
				if (alternativas.get(4).equals("53E")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil. Parabéns!\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta!\n\n"
									+ "Feedback: Para o cálculo de número de base octal, para hexadecimal, basta utilizar as dicas anteriores para passar o número para decimal\n"
									+ "ou para binário, e depois passar para hexadecimal. Ainda, se você souber montar a tabela de números binários, fica mais fácil.Boa sorte!\n");
					contador = contador + 1;
				}
				break;

			default:
				System.out.println("Escolha inválida!\n");
				contador = contador + 1;
				break;
			}
		} while (!acertou && contador < 3);

		if (acertou) {
			System.out.println("Acertou na tentativa: " + contador);
		} else {
			sextaQuestao = 'i';
			System.out.println("Iih você usou todas as tentativas");
		}

		return sextaQuestao;
	}

