	}

	static char setimoDesafio1() {
		char setimaQuestao;
		int contador = 0;

		boolean acertou = false;

		List alternativas = new ArrayList();

		alternativas.add("1457");
		alternativas.add("1246");
		alternativas.add("1312");
		alternativas.add("1541");
		alternativas.add("1313");

		do {
			Collections.shuffle(alternativas);

			System.out
					.println("Dado o número 1 0 1 1 0 0 1 0 1 1, de base binária, qual o seu correspondente octal? ");

			System.out.println("a) " + alternativas.get(0));
			System.out.println("b) " + alternativas.get(1));
			System.out.println("c) " + alternativas.get(2));
			System.out.println("d) " + alternativas.get(3));
			System.out.println("e) " + alternativas.get(4));
			System.out.print("Escolha uma alternativa: ");

			setimaQuestao = entrada.next().toLowerCase().charAt(0);

			switch (setimaQuestao) {

			case 'a':
				if (alternativas.get(0).equals("1313")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta! Mas você ainda pode acertar! Segue algumas dicas:\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					contador = contador + 1;
				}
				break;

			case 'b':
				if (alternativas.get(1).equals("1313")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta! Mas você ainda pode acertar! Segue algumas dicas:\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					contador = contador + 1;
				}
				break;

			case 'c':
				if (alternativas.get(2).equals("1313")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta! Mas você ainda pode acertar! Segue algumas dicas:\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					contador = contador + 1;
				}
				break;

			case 'd':
				if (alternativas.get(3).equals("1313")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta! Mas você ainda pode acertar! Segue algumas dicas:\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					contador = contador + 1;
				}
				break;

			case 'e':
				if (alternativas.get(4).equals("1313")) {
					System.out
							.println("\nResposta correta!\nVocê acertou o código!\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					acertou = true;
					contador = contador + 1;
				} else {
					System.out
							.println("\nResposta incorreta! Mas você ainda pode acertar! Segue algumas dicas:\n"
									+ "Provavelmente você teve de fazer o cálculo à mão.\n\n"
									+ "Feedback: A conversão de octal para binário é feita convertendo dígito a dígito de octal em binário, da direita para a esquerda. \n"
									+ "Cada digito é convertido para um grupo de 3 bits.\n");
					contador = contador + 1;
				}
				break;

			default:
				System.out.println("Escolha inválida!");
				contador = contador + 1;
				break;
			}
		} while (!acertou && contador < 3);

		if (acertou) {
			System.out.println("Acertou na tentativa: " + contador);
		} else {
			setimaQuestao = 'i';
			System.out.println("Iih você usou todas as tentativas");
		}

		return setimaQuestao;
	}

	static int interacao18(String nome) {
		int retornointeracao18;
		System.out
				.println("\nTABELA DE PISTAS:\nBILHETE\nPADARIA \nRELACIONAMENTO DE JOE COM PREFEITO\n\n"
						+ "A padaria serve como lavagem de dinheiro, que Joe comanda, como se fosse os olhos do prefeito. Joe poderia ser o principal \n"
						+ "suspeito das mortes dos acidentes, que nunca foram publicados, enquanto o prefeito limpava seus rastros. O principal: \n"
						+ "Joe deu o contato de seu irmão para o prefeito, assim sabemos que Patrick escondia os cadáveres.\n\n"
						+ "O prefeito, na verdade, era o mandante de toda a farsa. Os irmãos órfãos, Patrick e Joe Clin, eram apenas os executores.\n\n"
						+ "Karl Rahnner vai à polícia, leva o programa feito por "
						+ nome
						+ ", entregam o prefeito com todos seus comparsas.\n\n"
						+ "Agora nossa história se desenrola; a padaria fecha, o prefeito vai preso para depor e Joe some no meio do nada.\n");
		System.out.println("1- 'Agora é a hora da verdade'!\n"
				+ "2- 'Não podemos errar mais'!\n");
		retornointeracao18 = entrada.nextInt();
		return retornointeracao18;
	}

