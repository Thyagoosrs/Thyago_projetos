//Versão Final do Game, testada, funcionando!!

package ProjetoIntegrador;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;
import java.util.Random;
import java.util.Scanner;

public class DarkCasesOfMiddletown {
	public static void main(String[] args) {
		char entrar, saida, sair = 0;
		int retornoMenu;

		// apresentaçao inicial e identificacao do jogador
		String nome = inicio();

		do {
			// jogador escolhe opcoes do menu
			retornoMenu = menu();
			// a partir do que o usuario digitar ira abrir uma das opcoes abaixo
			switch (retornoMenu) {
			// instrucao: regras do jogo, como funciona e suas ordens
			case 1:
				instrucao();
				break;
			case 2:
				// o inicio da historia sera apresentada e um convite para ser
				// detetive será feito
				char fase1 = introducao(nome);
				if (fase1 == 's') {
					// caso o jogador queira iniciar o jogo, sera contada a
					// historia que antecede a primeira questao do jogo
					introPrimeiroDesafio();
					// apos a historia a ser contada, as questoes serao
					// sorteadas para o jogador responder
					Random sorteio1 = new Random(1);
					int respPrimeiroDesafio = primeiroDesafio1();
					// respPrimeiroDesafio = primeiroDesafio2();
					if (respPrimeiroDesafio == 'i') {
						gameOver();
						System.exit(sair);
					} else {
						interacao1(nome);
						interacao2(nome);
						interacao3(nome);
						interacao4(nome);
						interacao5(nome);
						interacao6(nome);
						introSegundoDesafio(nome);
						Random sorteio2 = new Random(1);
						int respSegundoDesafio = segundoDesafio1();
						// respSegundoDesafio = segundoDesafio2();
						if (respSegundoDesafio == 'i') {
							gameOver();
							System.exit(sair);
						} else {
							interacao7(nome);
							interacao8(nome);
							introTerceiroDesafio(nome);
							Random sorteio3 = new Random(1);
							int respTerceiroDesafio = terceiroDesafio1();
							// respTerceiroDesafio = terceiroDesafio2();
							if (respTerceiroDesafio == 'i') {
								gameOver();
								System.exit(sair);
							} else {
								interacao9(nome);
								interacao10(nome);
								interacao11(nome);
								interacao12(nome);
								interacao13(nome);
								interacao14(nome);
								Random sorteio4 = new Random(1);
								int respQuartoDesafio = quartoDesafio1();
								// respQuartoDesafio = quartoDesafio2();
								if (respQuartoDesafio == 'i') {
									gameOver();
									System.exit(sair);
								} else {
									interacao15(nome);
									interacao16(nome);
									introQuintoDesafio(nome);
									Random sorteio5 = new Random(1);
									int respQuintoDesafio = quintoDesafio1();
									// respQuintoDesafio = quintoDesafio2();
									if (respQuintoDesafio == 'i') {
										gameOver();
										System.exit(sair);
									} else {
										interacao17(nome);
										introSextoDesafio(nome);
										int respSextoDesafio = sextoDesafio1();
										// int respSextoDesafio =
										// sextoDesafio2();
										if (respSextoDesafio == 'i') {
											gameOver();
											System.exit(sair);
										} else {
											introSetimoDesafio(nome);
											int respSetimoDesafio = setimoDesafio1();
											// respSetimoDesafio =
											// setimoDesafio2();
											if (respSetimoDesafio == 'i') {
												gameOver();
												System.exit(sair);
											} else {
												interacao18(nome);
												interacao19(nome);
												introOitavoDesafio(nome);
												int respoitavoDesafio = oitavoDesafio1();
												// respoitavoDesafio =
												// oitavoDesafio2();
												if (respoitavoDesafio == 'i') {
													gameOver();
													System.exit(sair);
												} else {
													finalizacaoJogo(nome);
												}
											}
										}
									}
								}
							}
						}
					}
				} else {
					saida = desistenciaJogador();

					if (saida == 'n') {
						System.out
								.println("Você será direcionado novamente para o menu");
					} else if (saida == 's') {
						gameOver();
						System.exit(sair);
					}
				}
				break;
			// creditos: aqui o usuario sabera quem desenvolvou o jogo
			case 3:
				creditos();
				break;
			// sair do jogo
			case 4:
				gameOver();
				System.exit(sair);
				break;
			// nenhuma das opcoes citadas
			default:
				System.out.println("Comando inválido");
			}
		} while (retornoMenu != 4);
		System.exit(sair);
	}

	static Scanner entrada = new Scanner(System.in);

	static String inicio() {
		String nomeJogador;
		System.out
				.println("  _____             _       _____                            __   __  __ _     _     _ _      _                      \n"
						+ " |  __ \\           | |     / ____|                          / _| |  \\/  (_)   | |   | | |    | |                     \n"
						+ " | |  | | __ _ _ __| | __ | |     __ _ ___  ___ ___    ___ | |_  | \\  / |_  __| | __| | | ___| |_ _____      ___ __  \n"
						+ " | |  | |/ _` | '__| |/ / | |    / _` / __|/ _ / __|  / _ \\|  _| | |\\/| | |/ _` |/ _` | |/ _ | __/ _ \\ \\ /\\ / | '_ \\ \n"
						+ " | |__| | (_| | |  |   <  | |___| (_| \\__ |  __\\__ \\ | (_) | |   | |  | | | (_| | (_| | |  __| || (_) \\ V  V /| | | |\n"
